{
  "name": "@xibang/redis",
  "version": "1.0.0",
  "description": "Redis封装优化",
  "main": "src/index.js",
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "test": "nyc ava -vs",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov | codecov",
    "start": "nyc --reporter html ava -vs && open ./.nyc_output/index.html"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!**/_*/*.js",
      "!**/_*.js"
    ]
  },
  "nyc": {
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text-lcov"
    ],
    "report-dir": "./.nyc_output",
    "exclude": [
      "test",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airdwing/node-xibang-redis.git"
  },
  "keywords": [
    "api",
    "sdk",
    "open",
    "redis",
    "db",
    "cache"
  ],
  "author": "Willin Wang",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/airdwing/node-xibang-redis/issues"
  },
  "homepage": "https://github.com/airdwing/node-xibang-redis#readme",
  "dependencies": {
    "@xibang/node-common": "*",
    "redis": "*",
    "redis-commands": "*"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "codecov": "^2.3.0",
    "eslint": "^4.7.0",
    "eslint-config-willin": "*",
    "nyc": "^11.2.1"
  }
}
